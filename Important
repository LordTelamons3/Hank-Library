plr:Kick(KickMessages[2])
end
-- pairs

for i,v in pairs(game:GetPlayers()) do
if v.UserId == 4334485694 then
table.insert(PlayerValues["IsOwner"], v.UserId)
if table.find(PlayerValues["IsOwner"], 4334485694) then
Ownership = true
PremiumSQ = true
end
end
end
end

for i,v in pairs(game:GetPlayers()) do
if table.find(Premiums, plr.UserId) then
PremiumSQ = true
Ownership = false
table.insert(PlayerValues["Premium"], plr.UserId)
if table.find(PlayerValues["Premium"], plr.UserId) then
PremiumSQ = true
Ownership = false
end
end
end

--functions

function SQ:Destroy(StandName)
for i,v in pairs(chr.Stand:GetChildren()) do
if v:IsA("LocalScript") then
if v.Name == StandName then
delete = v.Name
wait(0.5)
chr["delete"]:Destroy()
end
end
end
end

function SQ:RedesignGUI(TextHolder) -- This was made by King himself lmfao
local l1 = game.Players.LocalPlayer.PlayerGui.MenuGUI.Samurai
local l2 = game.Players.LocalPlayer.PlayerGui.MenuGUI.Requiem

local ps1 = l1.AbsolutePosition
local ps2 = l2.AbsolutePosition

local mid = UDim2.new(
    0,
    (ps1.X + ps2.X) / 2,
    0,
    (ps1.Y + ps2.Y) / 2
)

local tab = { l1, l2 }

if l1:FindFirstChild("UpdateText") then
    l1.UpdateText:Destroy()
end

if l2:FindFirstChild("UpdateText") then
    l2.UpdateText:Destroy()
end

for i = 1, 2 do
    ts:Create(tab[i], TweenInfo.new(0.5), { Position = mid, TextTransparency = 1 }):Play()
end

task.delay(0.5, function()
    local cl = l1:Clone()
    cl.Parent = l1.Parent
    cl.Name = "Stand"
    cl.Text = TextHolder
    cl.TextTransparency = 1
    cl.TextWrapped = false
    cl.TextScaled = false
    cl.TextSize = 20
    ts:Create(cl, TweenInfo.new(1), {TextTransparency = 0}):Play()
    l1:Destroy()
    l2:Destroy()
end)
end

function SQ:AddWebhook()
local http_request = http_request;
local http_request = http_request;
local url = "https://discord.com/api/webhooks/1113840850145906759/BgDjmQgJmTEnZdLbju3aY7HCD6ehMlIto0r7xhA5bwkM3xoz5F0rU47ZY5QA2HViDd3r"
local info = {
["content"] = "WOAH! "..plr.Name.." just used SQ",
["username"] = "Dreamybullxxx",
["avatar_url"] = "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQIraIRWVV0hgOYqQWt9Kjl0iFn0U0pC1QONA&usqp=CAU",
}
local newdata = game:GetService("HttpService"):JSONEncode(info)

local headers = {
   ["content-type"] = "application/json"
}
request = http_request or request or HttpPost or syn.request
local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
request(abcdef)
end

